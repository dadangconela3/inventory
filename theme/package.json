    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@popperjs/core": "^2.11.8",
    "accordion-js": "^3.4.1",
    "apexcharts": "^5.3.6",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.19",
    "filepond": "^4.32.11",
    "filepond-plugin-image-preview": "^4.6.12",
    "flatpickr": "^4.6.13",
    "gridjs": "^6.2.0",
    "highlight.js": "^11.11.1",
    "quill": "^2.0.3",
    "simplebar": "^6.3.3",
    "sortablejs": "^1.15.6",
    "swiper": "^12.0.3",
