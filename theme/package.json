{
  "name": "lineone",
  "version": "3.2.2",
  "description": "Multipurpose Admin UI Kit based on Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod && npm run hot",
    "dev": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production"
  },
  "keywords": [
    "Tailwind CSS",
    "Admin UI"
  ],
  "author": "PiniaStudio",
  "devDependencies": {
    "autoprefixer": "^10.4.23",
